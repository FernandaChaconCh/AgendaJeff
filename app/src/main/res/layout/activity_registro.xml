<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Registro" android:background="@drawable/ic_background">

    <TextView
            android:text="@string/registro"
            android:layout_width="162dp"
            android:layout_height="34dp"
            android:id="@+id/textView3" android:textColor="#FFFFFF"
            android:textSize="18sp" app:fontFamily="@font/allerta_stencil" android:layout_marginTop="70dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="124dp" android:layout_marginStart="124dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="125dp"
            android:layout_marginRight="125dp" android:layout_marginBottom="83dp"
            app:layout_constraintBottom_toTopOf="@+id/nombre"/>
    <TextView
            android:text="@string/matricula_registro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" android:textColor="#FFFFFF"
            app:fontFamily="@font/denk_one" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="53dp" android:layout_marginStart="53dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="298dp" android:layout_marginRight="298dp" android:layout_marginBottom="23dp"
            app:layout_constraintBottom_toTopOf="@+id/matricula"/>
    <TextView
            android:text="@string/carrera_registro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:textColor="#FFFFFF"
            app:fontFamily="@font/denk_one" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="45dp" android:layout_marginStart="45dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="306dp" android:layout_marginRight="306dp" android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toTopOf="@+id/licenciatura" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="@string/name_registro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" android:textColor="#FFFFFF"
            app:fontFamily="@font/denk_one"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="45dp"
            android:layout_marginStart="45dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="318dp"
            android:layout_marginRight="318dp" android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toTopOf="@+id/nombre"/>
    <EditText
            android:layout_width="327dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/matricula"
            android:hint="@string/matricula_registro2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="45dp" android:layout_marginStart="45dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="39dp" android:layout_marginRight="39dp" android:layout_marginBottom="38dp"
            app:layout_constraintBottom_toTopOf="@+id/textView5" android:autofillHints=""
            android:textColor="#FFFFFF"/>
    <EditText
            android:layout_width="327dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/nombre"
            android:hint="@string/name_registro2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="45dp" android:layout_marginStart="45dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="39dp" android:layout_marginRight="39dp" android:layout_marginBottom="44dp"
            app:layout_constraintBottom_toTopOf="@+id/textView4" android:textColor="#FFFFFF"/>
    <EditText
            android:layout_width="327dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/licenciatura"
            android:hint="@string/carrera_registro2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="45dp" android:layout_marginStart="45dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="39dp" android:layout_marginRight="39dp" android:layout_marginBottom="101dp"
            app:layout_constraintBottom_toTopOf="@+id/button_continuar" android:textColor="#FFFFFF"/>
    <Button
            android:text="@string/button_registro"
            android:layout_width="97dp"
            app:fontFamily="@font/allerta_stencil"
            android:layout_height="48dp"
            android:id="@+id/button_continuar" android:background="#3A8172"
            android:textColor="#FFFFFF" android:textSize="14sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="161dp" android:layout_marginStart="161dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="153dp"
            android:layout_marginRight="153dp" android:layout_marginBottom="93dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>